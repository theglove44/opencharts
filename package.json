{
  "name": "oss-charts",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "concurrently -n api,web -c green,blue \"pnpm --filter @oss-charts/api dev\" \"pnpm --filter @oss-charts/web dev\"",
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.44.1",
    "prettier": "^3.3.3",
    "svelte-eslint-parser": "^0.41.0",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  }
}
